{
  "name": "ai-browser-security",
  "version": "1.0.0",
  "description": "AI Browser Security - Unified Deployment",
  "private": true,
  "scripts": {
    "install:backend": "cd AWI/backend && npm install",
    "install:frontend": "cd AWI/frontend && npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "build:frontend": "cd AWI/frontend && npm run build",
    "build": "npm run install:all && npm run build:frontend",
    "start": "cd AWI/backend && npm start"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
